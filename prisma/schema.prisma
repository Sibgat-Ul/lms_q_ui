generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id       Int        @id @default(autoincrement())
  name     String     @db.VarChar(30)
  email     String     @db.VarChar(30)
  password String?    @db.VarChar(32)
  role     user_role?
}

enum user_role {
  student
  instructor
}
